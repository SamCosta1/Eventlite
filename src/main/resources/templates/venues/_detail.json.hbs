{
  "title": "EventLite Venue View",
  "_self": "{{self_link}}venues/{{venue.id}}",

  "id": "{{venue.id}}",
  "name": "{{venue.name}}",
  "capacity": {{venue.capacity}},
  "adressLine1": "{{venue.addressLine1}}",
  "adressLine2": "{{venue.addressLine2}}",
  "streetName": "{{venue.streetName}}",
  "city": "{{venue.city}}",
  "postcode": "{{venue.postcode}}",

  "events": [ {{#events}}
	  {
		"id": "{{id}}",
		"name": "{{name}}",
		"date": "{{date}}",
		"time": "{{time}}"
		"_self":"{{self_link}}events/{{id}}"
	  }{{#unless @last}},{{/unless}}{{/events}}
  ]
}